        "logs": "firebase functions:log"
    },
    "engines": {
        "node": "18"
    },
    "main": "lib/index.js",
    "dependencies": {
        "firebase-admin": "^12.0.0",
        "firebase-functions": "^4.5.0"
    },
    "devDependencies": {
        "typescript": "^5.0.0"
    },
    "private": true
}